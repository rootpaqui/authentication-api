{
  "name": "express-authentication-api",
  "version": "1.0.0",
  "description": "A simple Rest API for authentication in Nodejs & Express",
  "main": "server.js",
  "scripts": {
    "start": "node -r dotenv/config server.js",
    "dev": "node -r dotenv/config --watch server.js",
    "key:generate": "node scripts/generateSecretKey.js",
    "sequelize:migrate": "npx sequelize-cli db:migrate",
    "sequelize:migrate:undo": "npx sequelize-cli db:migrate:undo"
  },
  "keywords": [
    "nodejs",
    "express",
    "sequilize"
  ],
  "author": "PAQUI",
  "license": "ISC",
  "dependencies": {
    "bcrypt": "^5.1.1",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.21.1",
    "jsonwebtoken": "^9.0.2",
    "mysql2": "^3.11.3",
    "sequelize": "^6.37.4"
  },
  "devDependencies": {
    "sequelize-cli": "^6.6.2"
  }
}
